<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>edu.fa.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterController</servlet-name>
  	<display-name>RegisterController</display-name>
  	<description></description>
  	<servlet-class>edu.fa.controller.RegisterController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditProfileController</servlet-name>
  	<display-name>EditProfileController</display-name>
  	<description></description>
  	<servlet-class>edu.fa.controller.EditProfileController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>dfd</servlet-name>
  	<display-name>dfd</display-name>
  	<description></description>
  	<servlet-class>edu.fa.controller.dfd</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddContentController</servlet-name>
  	<display-name>AddContentController</display-name>
  	<description></description>
  	<servlet-class>edu.fa.controller.AddContentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ViewContentController</servlet-name>
  	<display-name>ViewContentController</display-name>
  	<description></description>
  	<servlet-class>edu.fa.controller.ViewContentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutController</servlet-name>
  	<display-name>LogoutController</display-name>
  	<description></description>
  	<servlet-class>edu.fa.controller.LogoutController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterController</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditProfileController</servlet-name>
  	<url-pattern>/edit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>dfd</servlet-name>
  	<url-pattern>/dfd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddContentController</servlet-name>
  	<url-pattern>/add-content</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ViewContentController</servlet-name>
  	<url-pattern>/view-content</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutController</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
</web-app>
